@Model  HomeIndexViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="container">
    <div class="row">
        <div class="col-md-4">
            @foreach (var item in Model.events)
            {
                <p>@item.Title</p>
            }
        </div>
        <div class="col-md-4">
            @foreach (var item in Model.jobs)
            {
                <p>@item.Title</p>
            }
        </div>
        <div class="col-md-4">
            @foreach (var item in Model.learnings)
            {
                <p>@item.Title</p>
            }
        </div>
    </div>
</div>